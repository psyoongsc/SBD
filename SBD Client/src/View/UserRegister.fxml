            <TextField fx:id="txtName" layoutX="66.0" layoutY="90.0" />
            <TextField fx:id="txtPhone" layoutX="66.0" layoutY="197.0" />
            <ComboBox fx:id="comboAddr1" layoutX="66.0" layoutY="232.0" onAction="#onComboAddr1Change" prefWidth="150.0" />
