<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<HBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.UserRegisterController">
   <children>
      <Pane prefHeight="300.0" prefWidth="400.0">
         <children>
            <TextField fx:id="txtID" layoutX="66.0" layoutY="14.0" />
